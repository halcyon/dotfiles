# 'C-/'
lctrl - 0x2C              : khd -w `printf "\x1f"`

lctrl + lalt - up         : khd -w `printf "\e[1;7A"`
lctrl + lalt - down       : khd -w `printf "\e[1;7B"`
lctrl + lalt - right      : khd -w `printf "\e[1;7C"`
lctrl + lalt - left       : khd -w `printf "\e[1;7D"`

lctrl + lshift - d        : khd -w `printf "\e[68;6"`
lctrl + lshift - a        : khd -w `printf "\e[65;6"`
lctrl + lalt + lshift - v : khd -w `printf "\e[86;8"`

lalt + lshift - up        : khd -w `printf "\e[1;4A"`
lalt + lshift - down      : khd -w `printf "\e[1;4B"`
lalt + lshift - right     : khd -w `printf "\e[1;4C"`
lalt + lshift - left      : khd -w `printf "\e[1;4D"`
